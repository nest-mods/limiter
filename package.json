{
  "name": "@nest-mods/limiter",
  "version": "1.0.0",
  "description": "limiter for nestjs (express)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "tslint -p tsconfig.json --fix",
    "prepare": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nest-mods/limiter.git"
  },
  "keywords": [],
  "author": {
    "name": "Diluka"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nest-mods/limiter/issues"
  },
  "homepage": "https://github.com/nest-mods/limiter#readme",
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "\\.(e2e-)?spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "@nestjs/common": ">=7",
    "@nestjs/core": ">=7"
  },
  "dependencies": {
    "express-rate-limit": "^5.1.3",
    "express-slow-down": "^1.3.1",
    "lodash": "^4.17.20",
    "rate-limit-redis": "^2.0.0",
    "tslib": "^2.0.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "@nestjs/graphql": "^7.6.0",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/testing": "^7.4.4",
    "@types/express-rate-limit": "^5.1.0",
    "@types/express-slow-down": "^1.3.0",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/node": "^10.17.29",
    "@types/rate-limit-redis": "^1.7.1",
    "@types/supertest": "^2.0.10",
    "apollo-server-express": "^2.17.0",
    "graphql-tag": "^2.11.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
